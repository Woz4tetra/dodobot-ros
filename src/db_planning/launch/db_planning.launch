<launch>
    <group ns="dodobot" >
        <node name="db_planning_central"      pkg="db_planning" type="central_planning.py" output="screen" required="true">
            <param name="debug_image_dir" value="$(find db_planning)/debug_images/" type="str"/>
            <param name="insert_sequence_path" value="$(find db_planning)/sequences/insert.csv" type="str"/>
            <param name="extract_sequence_path" value="$(find db_planning)/sequences/extract.csv" type="str"/>
            <param name="tag_name" value="target_rotated" type="str"/>
        </node>
        <node name="db_planning_chassis"      pkg="db_planning" type="chassis_planning.py" output="screen" required="true"></node>
        <node name="db_planning_front_loader" pkg="db_planning" type="front_loader_planning.py" output="screen" required="true"></node>
    </group>
    <include file="$(find db_planning)/launch/tag_conversion.launch"/>
</launch>
