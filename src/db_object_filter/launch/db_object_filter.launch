<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <group ns="dodobot" >
        <node name="db_object_filter" pkg="db_object_filter" type="db_object_filter_node" output="screen">
            <param name="model_name" value=""/>
            <param name="model_path" value="/home/ben/object-recognition/jetson-inference/python/training/detection/ssd/models/dodobot_objects/mb2-ssd-lite.onnx"/>
            <param name="prototxt_path" value=""/>
            <param name="class_labels_path" value="/home/ben/object-recognition/jetson-inference/python/training/detection/ssd/models/dodobot_objects/labels.txt"/>
            <param name="input_blob" value="input_0"/>
            <param name="output_cvg" value="scores"/>
            <param name="output_bbox" value="boxes"/>
            <param name="overlay_flags" value="box,labels,conf"/>
            <param name="mean_pixel_value" value="0.0"/>
            <param name="threshold" value="0.3"/>

            <param name="depth_topic"           value="/camera/aligned_depth_to_color/image_raw"/>
            <!-- <param name="depth_topic"           value="/camera/depth/image_rect_raw"/> -->
            <param name="depth_info_topic"      value="/camera/aligned_depth_to_color/camera_info"/>
            <!-- <param name="depth_info_topic"      value="/camera/depth/camera_info"/> -->

            <param name="color_topic"     value="/camera/color/image_raw"/>
            <param name="color_info_topic"     value="/camera/color/camera_info"/>

            <param name="approx_sync"           value="false"/>
            <param name="publish_debug_image"     value="true"/>

            <rosparam param="detectnet_labels">
                - BACKGROUND
                - cozmo_cube
                - blue_cut_sphere
                - red_cut_sphere
                - blue_low_bin
                - red_low_bin
                - blue_cube
                - red_cube
            </rosparam>

            <rosparam param="detectnet_marker_colors">
                BACKGROUND: [0.0, 0.0, 0.0, 1.0]
                cozmo_cube: [0.95, 1.0, 1.0, 1.0]
                blue_cut_sphere: [0.14, 0.75, 1.0, 1.0]
                red_cut_sphere: [1.0, 0.2, 0.13, 1.0]
                blue_low_bin: [0.13, 0.24, 0.52, 1.0]
                red_low_bin: [0.52, 0.2, 0.14, 1.0]
                blue_cube: [0.14, 0.75, 1.0, 1.0]
                red_cube: [1.0, 0.2, 0.13, 1.0]
            </rosparam>

            <rosparam param="detectnet_z_depth_estimations">
                BACKGROUND: 0.0
                cozmo_cube: 0.04
                blue_cut_sphere: 0.09
                red_cut_sphere: 0.09
                blue_low_bin: 0.116
                red_low_bin: 0.116
                blue_cube: 0.08
                red_cube: 0.08
            </rosparam>

        </node>
    </group>
</launch>
